<?xml version="1.0"?>
<package >
  <metadata>
    <id>Delegate.SharePoint.App.Template</id>
    <version>1.0.0</version>
    <title>Delegate's SharePoint app template structure</title>
    <authors>Jannik Rasmussen</authors>
    <owners>Jannik Rasmussen</owners>
    <licenseUrl>http://opensource.org/licenses/ms-pl.html</licenseUrl>
