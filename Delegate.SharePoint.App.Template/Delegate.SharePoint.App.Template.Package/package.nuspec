    <releaseNotes>Initial release.</releaseNotes>
    <copyright>Copyright 2015</copyright>
    <tags>Delegate Delegateas SharePoint App SPA Single Page Application Angular Angularjs TypeScript Template Structure Sample</tags>
    <dependencies>
      <dependency id="AngularJS.Core" version="[1.4.3,2.0.0)" />
      <dependency id="angularjs.TypeScript.DefinitelyTyped" version="[4.0.7,5.0.0)" />
      <dependency id="jquery.TypeScript.DefinitelyTyped" version="[2.2.6, 2.5.0)" />
      <dependency id="microsoft-ajax.TypeScript.DefinitelyTyped" version="[0.0.1,1.0.0)" />
      <dependency id="sharepoint.TypeScript.DefinitelyTyped" version="[0.1.8,1.0.0)" />
